    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.5.1",
    "express": "^4.17.1",
    "express-handlebars": "^5.3.1",
    "express-mysql-session": "^2.1.6",
    "express-session": "^1.17.1",
    "express-session-sequelize": "^2.3.0",
    "fs": "^0.0.1-security",
    "if-env": "^1.0.4",
